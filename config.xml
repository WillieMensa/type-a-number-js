<?xml version="1.0" encoding="utf-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" 
	id="ar.com.ingverger.typeanumber" version="0.0.1" 
	xmlns:cdv="http://cordova.apache.org/ns/1.0"
	version     = "1.0.0" >
	<name>Ejemplo con type a number</name>
	<description>
		Ejemplo para phonegap build docs.
	</description>
	<author email="ingverger@gmail.com" href="http://www.ingverger.com.ar">
		Willie Verger juegos de ingenio
	</author>
	<content src="index.html" />
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
	  <allow-intent href="itms:*" />
	  <allow-intent href="itms-apps:*" />
	</platform>
</widget>